	width:99%;
	overflow:hidden;         
	background:url(../images/tabbg_bg.png) repeat-x left bottom;
	font-size:12px;	
}
.off {
	color: #333;
	height: 21px;
	margin:1px 4px 0px 4px;
	_margin:3px 4px 0px 4px;/*设置上边距1px,以得到off的下边框与tabdiv下边线重合*/
	line-height: 21px;
	float: left;
	background:url(../images/tabbg12.png) no-repeat right top;/*****背景图片与on一致，均为tabbg.png*/
	cursor: pointer;
}
.off span{
	background:url(../images/tabbg1.png) no-repeat left top;/*****背景图片与on一致，均为tabbg.png*/
	cursor: pointer;
	padding:0 10px;
	display:block;
}
.on {
	color: #333333;
	height: 23px;
	margin:0px 4px 0px 4px;
	line-height: 23px;
	float: left;
	background:url(../images/tabbg22.png) no-repeat right top;/*****背景图片与off一致，均为tabbg.png*/
	
	cursor: pointer;
}
.on span{
	background:url(../images/tabbg2.png) no-repeat left top;
	
	cursor: pointer;
	padding:0 10px;
	display:block;
}
